  </urls>
  <releaseDate>2017-03-06</releaseDate>
  <version>1.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>4.6,4.7</ver>
  </compatibility>
  <comments></comments>
  <civix>
    <namespace>CRM/Birthyear</namespace>
  </civix>
</extension>
